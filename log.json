{
  "console": {
    "enable": false,
    "level": "FINE"
  },
  "files": [
    {
      "enable": true,
      "level": "FINE",
      "filename": "./logs/app.log",
      "category": "fileLogs",
      "pattern": "[%D %T] [%C] [%L] (%S) %M",
      "rotate": true,
      "maxsize": "2M",
      "daily": true,
      "maxlines": "10K",
      "sanitize": false
    }
  ]
}
